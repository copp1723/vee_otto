{
  "name": "vee-otto",
  "version": "1.0.0",
  "description": "AI-powered browser automation framework for vAuto inventory management",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node",
    "start": "node dist/src/server.js",
    "start:dev": "ts-node src/server.ts",
    "vauto:standard": "ts-node scripts/run-standard-vehicle-processing.ts",
    "vauto:enhanced": "ts-node scripts/run-enhanced-vehicle-processing.ts",
    "vauto:hybrid": "ts-node scripts/run-hybrid-vehicle-processing.ts",
    "vauto:full-workflow": "ts-node scripts/run-full-workflow.ts",
    "vauto:manual": "HEADLESS=false SLOW_MO=1000 ts-node scripts/run-standard-vehicle-processing.ts",
    "vauto:enhanced-manual": "HEADLESS=false SLOW_MO=1000 ts-node scripts/run-enhanced-vehicle-processing.ts",
    "vauto:hybrid-manual": "HEADLESS=false SLOW_MO=1000 ts-node scripts/run-hybrid-vehicle-processing.ts",
    "vauto:session-hybrid": "ts-node scripts/run-hybrid-with-session-persistence.ts",
    "test:factory-equipment": "ts-node scripts/test-factory-equipment-click.ts",
    "test:factory-simple": "ts-node scripts/test-factory-equipment-simple.ts",
    "build:dashboard": "webpack --config webpack.config.js",
    "serve:dashboard": "node scripts/serve-dashboard.js",
    "dashboard": "npm run build:dashboard && npm run serve:dashboard",
    "test:anomaly": "ts-node tests/unit/IntelligentAnomalyDetector.test.ts",
    "reports:view": "open ./reports/",
    "reports:clean": "rm -rf ./reports/*"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.9.0",
    "@types/nodemailer": "^6.4.14",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.10.0",
    "concurrently": "^9.2.0",
    "copy-webpack-plugin": "^13.0.0",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.2.0",
    "html-webpack-plugin": "^5.5.3",
    "jsonwebtoken": "^9.0.2",
    "mini-css-extract-plugin": "^2.7.6",
    "ml-regression": "^6.3.0",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "playwright": "^1.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.5.0",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "style-loader": "^3.3.3",
    "tesseract.js": "^6.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "twilio": "^5.7.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "tsx": "^4.20.3"
  },
  "keywords": [
    "automation",
    "browser",
    "playwright",
    "scraping",
    "2fa",
    "dashboard",
    "react",
    "vauto",
    "inventory-management"
  ],
  "author": "AI Agent System",
  "license": "MIT"
}
