{
  "name": "vee-otto",
  "version": "1.0.0",
  "description": "Generic browser automation framework for web scraping and interaction with Operations Dashboard",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.production.json",
    "build:all": "npm run build && npm run dashboard:build",
    "start": "node build/server.js",
    "automation": "ts-node scripts/run-automation.ts",
    "automation:help": "ts-node scripts/run-automation.ts --help",
    "automation:config": "ts-node scripts/run-automation.ts --create-config",
    "dashboard:dev": "webpack serve --mode development",
    "dashboard:build": "webpack --mode production --progress",
    "dashboard:prod": "node scripts/dashboard-server.js",
    "server:dev": "ts-node src/server.ts",
    "server:build": "tsc && node build/server.js",
    "visual": "ts-node src/visualBrowserRunner.ts",
    "render:build": "bash scripts/render-build.sh",
    "render:start": "node build/server.js",
    "test:email": "ts-node tests/test-email-connection.ts",
    "test:mailgun": "ts-node tests/test-mailgun.ts",
    "test:reliability": "ts-node tests/test-hybrid-reliability.ts",
    "test:vauto": "ts-node tests/test-vauto-flow.ts",
    "vauto:mockup": "node tests/serve-vauto-mockup.js",
    "vauto:mockup-test": "npx ts-node tests/run-vauto-mockup-with-monitoring.ts test",
    "vauto:mockup-manual": "npx ts-node tests/run-vauto-mockup-with-monitoring.ts manual",
    "vauto:mockup-watch": "npx ts-node tests/run-vauto-mockup-with-monitoring.ts both",
    "vauto:mockup:start": "node tests/start-vauto-mockup.js",
    "vauto:mockup:test": "ts-node tests/test-vauto-mockup.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:automation-flow": "ts-node scripts/test-automation-flow.ts",
    "vauto": "ts-node scripts/run-vauto.ts",
    "vauto:once": "ts-node scripts/run-vauto.ts --once",
    "vauto:test": "TEST_MODE=true ts-node scripts/run-vauto.ts --once",
    "vauto:safe": "./scripts/test-vauto-safely.sh",
    "experimental:vauto": "EXPERIMENTAL_MODE=true ts-node scripts/run-vauto.ts --once",
    "test:2fa": "node test-2fa-flow.js",
    "webhook:dev": "cd deployments/webhook-handler && npm run dev",
    "webhook:build": "cd deployments/webhook-handler && npm run build"
  },
  "dependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fs-extra": "^11.0.4",
    "@types/imapflow": "^1.0.23",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mailparser": "^3.4.4",
    "@types/node": "^20.9.0",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.0",
    "fs-extra": "^11.2.0",
    "fuzzball": "^2.1.2",
    "html-webpack-plugin": "^5.5.3",
    "imap": "^0.8.19",
    "imapflow": "^1.0.191",
    "jsonwebtoken": "^9.0.2",
    "mailparser": "^3.6.5",
    "mini-css-extract-plugin": "^2.7.6",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "p-queue": "^8.1.0",
    "p-retry": "^6.2.1",
    "playwright": "^1.40.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.5.0",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "sqlite3": "^5.1.7",
    "style-loader": "^3.3.3",
    "tesseract.js": "^6.0.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "twilio": "^5.7.3",
    "typescript": "^5.2.2",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/imap": "^0.8.40",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "keywords": [
    "automation",
    "browser",
    "playwright",
    "scraping",
    "2fa",
    "dashboard",
    "react"
  ],
  "author": "AI Agent System",
  "license": "MIT"
}
